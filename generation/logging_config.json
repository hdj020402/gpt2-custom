{
    "formatters": {
      "vllm": {
        "class": "vllm.logging_utils.NewLineFormatter",
        "datefmt": "%m-%d %H:%M:%S",
        "format": "%(levelname)s %(asctime)s %(filename)s:%(lineno)d] %(message)s"
      }
    },
    "handlers": {
      "vllm": {
        "class" : "logging.StreamHandler",
        "formatter": "vllm",
        "level": "INFO",
        "stream": "ext://sys.stdout"
      }
    },
    "loggers": {
      "vllm": {
        "handlers": ["vllm"],
        "level": "DEBUG",
        "propagate": false
      },
      "vllm.example_noisy_logger": {
        "propagate": false
      }
    },
    "version": 1
  }